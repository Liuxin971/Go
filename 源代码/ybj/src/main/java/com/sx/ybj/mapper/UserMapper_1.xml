<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sx.ybj.mapper.UserMapper_1">
    <select id="loadUserByUsername" resultType="com.sx.ybj.pojo.User">
        SELECT * FROM user WHERE username=#{username}
    </select>
    <select id="getAllRole" resultType="com.sx.ybj.pojo.Roles">
        select * from roles;
    </select>

</mapper>